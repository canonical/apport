#: ../bin/apport-cli.py:309 ../gtk/apport-gtk.ui.h:25 ../kde/apport-kde.py:353
msgid "Uploading problem information"
msgstr "Nahrávam informácie o probléme"

#: ../bin/apport-cli.py:310
msgid ""
"The collected information is being sent to the bug tracking system.\n"
"This might take a few minutes."
msgstr ""
"Zhromaždené informácie sa posielajú systému na sledovanie chýb.\n"
"Môže to trvať pár minút."

#: ../bin/apport-cli.py:360
msgid "&Done"
msgstr "&Dokončené"

#: ../bin/apport-cli.py:366
msgid "none"
msgstr "žiadny"

#: ../bin/apport-cli.py:367
#, python-format
