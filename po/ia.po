#, python-format
msgid "&Send report (%s)"
msgstr "&S Mandar le reporto (%s)"

#: ../bin/apport-cli.py:222
msgid "&Examine locally"
msgstr "&Examinar localmente"

#: ../bin/apport-cli.py:226
msgid "&View report"
msgstr "&Vider le reporto"

